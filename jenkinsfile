pipeline {
    agent any
    
    stages {
        stage('Install dependencies') {
            steps {
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
               sh 'echo "Testing application.."'
            }
        }
        stage('Deploy nodejs') {
            steps {
               sh 'echo "Deploying ...."'
            }
        }
    }
}